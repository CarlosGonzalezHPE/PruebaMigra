<transformations xmlns="http://www.hp.com/usage/datastruct/transform"
    targetNamespace="http://www.hp.com/usage/datastruct/transform/SNME_TestData_to_XFD_TestData">

    <import prefix="t" ns="http://www.hp.com/usage/datastruct/transform" />
    <import prefix="tnme" ns="http://www.hp.com/usage/datastruct/transform/nme" />

    <import prefix="xfd" ns="http://www.hp.com/usage/datastruct/xfd/TestData" />
    <import prefix="snme" ns="http://www.hp.com/usage/nme/nmeschema/TestData" />
    <import prefix="nme" ns="http://www.hp.com/usage/nme/nmeschema" />

    <transformer id="SNME_TestData_to_XFD_TestData" from="snme:TestData" to="xfd:TestData">
        <transform input="intField" output="intField">
            <use-transformer from="int" to="int" class="t:IntToIntCopy" />
        </transform>
        <transform input="byteArrayField" output="byteArrayFieldLength">
            <use-transformer from="nme:byte-array" to="int" class="NMEByteArrayLength" />
        </transform>
        <transform input="byteArrayField" output="byteArrayField">
            <use-transformer from="nme:byte-array" to="byte[]" class="tnme:ByteArrayWrapperToByteArray" />
        </transform>
        <transform input="structField" output="structField">
            <use-transformer from="snme:StructType" to="xfd:StructType" class="SNME_StructType_to_XFD_StructType" />
        </transform>
    </transformer>

    <transformer id="SNME_StructType_to_XFD_StructType" from="snme:StructType" to="xfd:StructType">
        <transform input="structByteField" output="structByteField">
            <use-transformer from="byte" to="byte" class="t:ByteToByteCopy" />
        </transform>
        <transform input="structStringField" output="structStringFieldLength">
            <use-transformer from="string" to="int" class="t:StringLength" />
        </transform>
        <transform input="structStringField" output="structStringField">
            <use-transformer from="string" to="string" class="t:StringToStringCopy" />
        </transform>
    </transformer>

    <transformer id="NMEByteArrayLength" from="nme:byte-array" to="int">
        <chain>
        <use-transformer from="nme:byte-array" to="byte[]" class="tnme:ByteArrayWrapperToByteArray"/>
        <use-transformer from="byte[]" to="int" class="t:ByteArrayLength"/>
        </chain>
    </transformer>
</transformations>
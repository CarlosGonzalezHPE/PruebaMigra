<transformations xmlns="http://www.hp.com/usage/datastruct/transform"
    targetNamespace="http://www.hp.com/usage/datastruct/transform/XFD_helloworld_to_SNME_helloworld">

    <import prefix="t" ns="http://www.hp.com/usage/datastruct/transform" />

    <import prefix="xfd" ns="http://www.hp.com/usage/datastruct/xfd/helloworld" />
    <import prefix="snme" ns="http://www.hp.com/usage/nme/nmeschema/helloworld" />

    <transformer id="XFD_Message_to_SNME_Message" from="xfd:Message" to="snme:Message">
        <transform input="data" output="data">
            <use-transformer from="string" to="string" class="t:StringToStringCopy" />
        </transform>
    </transformer>
</transformations>
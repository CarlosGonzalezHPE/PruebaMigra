		</codec>
		<sequence>
			<!--<field id="reserved" type="byte" codec="Byte1" />-->
			<field id="protocolID" type="short" codec="Byte2" />
			<field id="dscpTosField" type="short" codec="UByte1" />
			<field id="dscpTosMask" type="short" codec="UByte1" />
			<field id="iPSourceAddress" type="byte[]" codec="IpV4" />
			<field id="iPSourceMask" type="byte[]" codec="IpV4" />
			<field id="iPDestinationAddress" type="byte[]" codec="IpV4" />
			<field id="iPDestinationMask" type="byte[]" codec="IpV4" />
			<field id="sourcePortStart" type="int" codec="UByte2" />
			<field id="sourcePortEnd" type="int" codec="UByte2" />
			<field id="destinationPortStart" type="int" codec="UByte2" />
			<field id="destinationPortEnd" type="int" codec="UByte2" />
			<field id="classifierID" type="int" codec="UByte2" />
			<field id="priority" type="byte" codec="Byte1" />
			<field id="activationState" type="short" codec="UByte1" />
			<field id="action" type="short" codec="UByte1" />
			<field id="reservedEnd" type="byte[]" codec="Byte3" />
		</sequence>
	</type>
	<type id="IPV6Classifier">
		<codec>
			<raw />
		</codec>
		<sequence>
			<field id="reserved" type="byte" codec="UBit4" />
			<field id="flags" type="byte" codec="UBit4" />
			<field id="tcLow" type="byte" codec="Byte1" />
			<field id="tcHigh" type="byte" codec="Byte1" />
			<field id="tcMask" type="byte" codec="Byte1" />
			<field id="flowLabel" type="long" codec="UByte4" />
			<field id="nextHeaderType" type="short" codec="Byte2" />
			<field id="sourcePrefixLength" type="byte" codec="Byte1" />
			<field id="destinationPrefixLength" type="byte" codec="Byte1" />
			<field id="iPV6SourceAddress" type="byte[]" codec="IpV6" />
			<field id="iPV6DestinationAddress" type="byte[]" codec="IpV6" />
			<field id="sourcePortStart" type="int" codec="UByte2" />
			<field id="sourcePortEnd" type="int" codec="UByte2" />
			<field id="destinationPortStart" type="int" codec="UByte2" />
			<field id="destinationPortEnd" type="int" codec="UByte2" />
			<field id="classifierID" type="int" codec="UByte2" />
			<field id="priority" type="byte" codec="Byte1" />
			<field id="activationState" type="short" codec="UByte1" />
			<field id="action" type="short" codec="UByte1" />
			<field id="reservedEnd" type="byte[]" codec="Byte3" />
		</sequence>
	</type>

	<type id="FlowSpec">
		<codec>
			<raw />
		</codec>
		<sequence>
			<field id="envelope" type="byte" codec="Byte1" />
			<field id="serviceNumber" type="byte" codec="Byte1" />
			<field id="reserveFirst" type="byte" codec="Byte1" />
			<field id="reserveSecond" type="byte" codec="Byte1" />
			<switch field="envelope">
				<case value="1">
					<field id="authEnvelope" type="FlowSpecEnvelope" />
				</case>
				<case value="3">
					<field id="authReservedEnvelope" type="AuthReservedEnvelope" />
				</case>
				<case value="7">
					<field id="authReservedCommittedEnvelope" type="AuthReservedCommittedEnvelope" />
				</case>
			</switch>
		</sequence>
	</type>
	<type id="AuthReservedEnvelope">
		<codec>
			<raw />
		</codec>
		<sequence>
			<field id="authorizedEnvelope" type="FlowSpecEnvelope" />

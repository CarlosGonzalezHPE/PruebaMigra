			<field id="forbiddenAttrMask" type="int" codec="Byte4" />
			<field id="attrAggregationRuleMask" type="int" codec="Byte4" />
			<field id="minBuffer" type="long" codec="UByte4" />
			<field id="targetBuffer" type="long" codec="UByte4" />
			<field id="maxBuffer" type="long" codec="UByte4" />
		</sequence>
	</type>


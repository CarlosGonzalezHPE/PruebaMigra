			<field id="peakDataRate" type="byte[]" codec="ByteArray4" />
			<field id="minPolicedUnit" type="int" codec="Byte4" />
			<field id="maxPacketSize" type="int" codec="Byte4" />
			<field id="rate" type="byte[]" codec="ByteArray4" />
			<field id="slackTerm" type="int" codec="Byte4" />
		</sequence>
	</type>

	<type id="DocSisServiceClass">
		<codec>
			<raw />
		</codec>
		<sequence>
			<field id="envelope" type="byte" codec="Byte1" />
			<field id="reserved3Bytes" type="Reserved3Bytes" />

			<field id="dscpTosOverwrite" type="short" codec="UByte1" />
			<field id="dscpTosMask" type="short" codec="UByte1" />
			<field id="sessionClassIDConfigurable" type="byte" codec="UBit4" />
			<field id="sessionClassIDPreemption" type="byte" codec="UBit1" />
			<field id="sessionClassIDPriority" type="byte" codec="UBit3" />
			<field id="timerT1" type="int" codec="UByte2" />
			<field id="timerT2" type="int" codec="UByte2" />
			<field id="timerT3" type="int" codec="UByte2" />
			<field id="timerT4" type="int" codec="UByte2" />

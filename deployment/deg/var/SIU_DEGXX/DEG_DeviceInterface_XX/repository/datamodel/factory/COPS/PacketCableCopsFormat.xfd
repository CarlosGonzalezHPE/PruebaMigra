	<type id="BestEffortService">
		<codec>
			<raw />
		</codec>
		<sequence>
			<field id="envelope" type="byte" codec="Byte1" />
			<field id="reserved3Bytes" type="Reserved3Bytes" />
			<switch field="envelope">
				<case value="1">
					<field id="authBESEnvelope" type="BestEffortServiceEnvelope" />
				</case>
				<case value="3">
					<field id="authReservedBESEnvelope" type="AuthReservedBESEnvelope" />
				</case>
				<case value="7">
					<field id="authReservedCommittedBESEnvelope" type="AuthReservedCommittedBESEnvelope" />
				</case>
			</switch>
		</sequence>

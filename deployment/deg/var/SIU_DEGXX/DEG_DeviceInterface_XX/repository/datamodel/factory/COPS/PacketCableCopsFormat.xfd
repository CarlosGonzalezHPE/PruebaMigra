<format xmlns="http://www.hp.com/usage/datastruct/xfd"
	targetNamespace="http://www.hp.com/usage/datastruct/xfd/PacketCableCopsFormat">

	<root type="PacketCableMessage" />

	<type id="PacketCableMessage" EOR="true">
		<codec>
			<raw size="length" />
		</codec>
		<sequence>
			<field id="version" type="byte" codec="UBit4" />
			<field id="flags" type="byte" codec="UBit4" />
			<field id="opCode" type="short" codec="UByte1" />
			<field id="clientType" type="int" codec="UByte2" />
			<field id="length" type="long" codec="UByte4" />
			<switch field="opCode">
				<case value="1">
					<field id="request" type="Request" />
				</case>
				<case value="2">
					<field id="decision" type="Decision" />
				</case>
				<case value="3">
					<field id="reportState" type="ReportState" />
				</case>
				<case value="4">
					<field id="deleteRequestState" type="DeleteRequestState" />
				</case>
				<case value="6">
					<field id="clientOpen" type="ClientOpen" />
				</case>
				<case value="7">
					<field id="clientAccept" type="ClientAccept" />
				</case>
				<case value="8">
					<field id="clientClose" type="ClientClose" />
				</case>
			</switch>
		</sequence>
	</type>

	<type id="Request">
		<codec>
			<tlv header-type="ValueHeader" tag-field="cNumCType"
				length-field="length" />
		</codec>
		<set>
			<field id="handle" type="Handle">
				<codec>
					<tlv code="257" optional="false" />
				</codec>
			</field>
			<field id="context" type="Context">
				<codec>
					<tlv code="513" optional="false" />
				</codec>
			</field>
			<field id="csiCType1" type="GateControlResponse">
				<codec>
					<tlv code="2305" optional="true" />
				</codec>
			</field>
			<field id="csiCType2" type="CSICType2">
				<codec>
					<tlv code="2306" valueCodec="StringUTF8Codec" optional="true" />
				</codec>
			</field>
		</set>
	</type>

	<type id="Decision">
		<codec>
			<tlv header-type="ValueHeader" tag-field="cNumCType"
				length-field="length" />
		</codec>
		<set>
			<field id="handle" type="Handle">
				<codec>
					<tlv code="257" optional="false" />
				</codec>
			</field>
			<field id="context" type="Context">
				<codec>
					<tlv code="513" optional="false" />
				</codec>
			</field>
			<field id="decisionCType1" type="DecisionCType1">
				<codec>
					<tlv code="1537" optional="false" />
				</codec>
			</field>
			<field id="decisionCType4" type="GateControlCommand">
				<codec>
					<tlv code="1540" optional="true" />
				</codec>
			</field>
			<field id="error" type="Error">
				<codec>
					<tlv code="2049" optional="true" />
				</codec>
			</field>
		</set>
	</type>
	<type id="ReportState">
		<codec>
			<tlv header-type="ValueHeader" tag-field="cNumCType"
				length-field="length" />
		</codec>
		<set>
			<field id="handle" type="Handle">
				<codec>
					<tlv code="257" optional="false" />
				</codec>
			</field>
			<field id="reportType" type="ReportType">
				<codec>
					<tlv code="3073" optional="false" />
				</codec>
			</field>
			<field id="csiCType1" type="GateControlResponse">
				<codec>
					<tlv code="2305" optional="true" />
				</codec>
			</field>
			<field id="csiCType2" type="CSICType2">
				<codec>
					<tlv code="2306" valueCodec="StringUTF8Codec" optional="true" />
				</codec>
			</field>
		</set>
	</type>

	<type id="DeleteRequestState">
		<codec>
			<tlv header-type="ValueHeader" tag-field="cNumCType"
				length-field="length" />
		</codec>
		<set>
			<field id="handle" type="Handle">
				<codec>
					<tlv code="257" optional="false" />
				</codec>
			</field>
			<field id="reason" type="Reason">
				<codec>
					<tlv code="1281" optional="false" />
				</codec>
			</field>
		</set>
	</type>


	<type id="ClientOpen">
		<codec>
			<tlv header-type="ValueHeader" tag-field="cNumCType"
				length-field="length" />
		</codec>
		<set>
			<field id="pepId" type="PEPID">
				<codec>
					<tlv code="2817" valueCodec="StringUTF8Codec" optional="false" />
				</codec>
			</field>
			<field id="versionInfo" type="PacketCableVersionInfo">
				<codec>
					<tlv code="2305" optional="true" />
				</codec>
			</field>
		</set>
	</type>

	<type id="ClientAccept">
		<codec>
			<tlv header-type="ValueHeader" tag-field="cNumCType"
				length-field="length" />
		</codec>
		<set>
			<field id="keepAliveTimer" type="KeepAliveTimer">
				<codec>
					<tlv code="2561" optional="false" />
				</codec>
			</field>
		</set>
	</type>

	<type id="ClientClose">
		<codec>
			<tlv header-type="ValueHeader" tag-field="cNumCType"
				length-field="length" />
		</codec>
		<set>
			<field id="error" type="Error">
				<codec>
					<tlv code="2049" optional="false" />
				</codec>
			</field>
		</set>
	</type>

	<type id="PacketCableVersionInfo">
		<codec>
			<tlv header-type="Header" tag-field="sNumSType" length-field="length" />
		</codec>
		<set>
			<field id="versionInfo" type="VersionInfo">
				<codec>
					<tlv code="4097" optional="false" />
				</codec>
			</field>
		</set>
	</type>


	<!-- Individual PacketCable XFD Types -->
	<type id="AMID">
		<codec>
			<raw />
		</codec>
		<sequence>
			<field id="applicationType" type="int" codec="UByte2" />
			<field id="applicationManagerTag" type="int" codec="UByte2" />
		</sequence>
	</type>
	<type id="SubIDIPV4">
		<codec>
			<raw />
		</codec>
		<sequence>
			<field id="subIdIpV4" type="byte[]" codec="IpV4" />
		</sequence>
	</type>
	<type id="SubIDIPV6">
		<codec>
			<raw />
		</codec>
		<sequence>
			<field id="subIdIpV6" type="byte[]" codec="IpV6" />
		</sequence>
	</type>
	<type id="GateID">
		<codec>
			<raw />
		</codec>
		<sequence>
			<field id="gateID" type="long" codec="UByte4" />
		</sequence>
	</type>
	<type id="GateSpec">

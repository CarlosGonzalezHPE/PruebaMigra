<?xml version="1.0" encoding="utf-8"?>
<format xmlns="http://www.hp.com/usage/datastruct/xfd"
    targetNamespace="http://www.hp.com/usage/datastruct/xfd/TestData">

    <root type="TestData" />


    <type id="TestData" EOR="true" >
    <codec>
        <raw />
    </codec>
        <sequence>
            <field id="intField" type="int" codec="Byte4" />
            <field id="byteArrayFieldLength" type="int" codec="Byte4" />
            <field id="byteArrayField" type="byte[]">
                <codec>
                    <raw size="byteArrayFieldLength" type ="byte[]" />
                </codec>
            </field>
            <field id="structField" type="StructType" />
        </sequence>
    </type>

    <type id="StructType" >
    <codec>
        <raw />
    </codec>
        <sequence>
            <field id="structByteField" type="byte" codec="Byte1" />
            <field id="structStringFieldLength" type="int" codec="Byte4" />
            <field id="structStringField" type="string" >
                <codec>
                    <raw type="string" charset="UTF8" size="structStringFieldLength" />
                </codec>
            </field>
        </sequence>
    </type>

    <codec id="Byte1" >
        <raw type="byte" size="1" unit="Byte" order="big-endian" unsigned="false" />
    </codec>

    <codec id="Byte4" >
        <raw type="int" size="4" unit="Byte" order="big-endian" unsigned="false" />
    </codec>
</format>
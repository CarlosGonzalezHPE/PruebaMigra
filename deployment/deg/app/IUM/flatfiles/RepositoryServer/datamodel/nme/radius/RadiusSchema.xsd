            <element name="AAARadiusMessage" type="tns:RadiusMessage" minOccurs="0"/>
            <element name="specifier" type="nme:string" minOccurs="0"/>
		</sequence>
		 </complexType>
		


	<complexType name="RadiusMessage">
		<sequence>
		    <element name="request" type="tns:RadiusRequest" />
		    <element name="answer" type="tns:RadiusAnswer" />
		    <element name="isAnswer" type="nme:boolean" />
		</sequence>
	    </complexType>
	    
	     
     <complexType name="RadiusRequest">
